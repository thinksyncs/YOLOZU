{
  "dataset": {
    "root": "../data/coco128",
    "split": "train2017",
    "format": "yolo"
  },
  "model": {
    "num_classes": 80,
    "hidden_dim": 384,
    "num_queries": 300,
    "use_uncertainty": false,
    "backbone_name": "cspresnet",
    "stem_channels": 64,
    "backbone_channels": [128, 256, 512],
    "stage_blocks": [2, 3, 4],
    "use_sppf": true,
    "use_level_embed": true,
    "num_encoder_layers": 2,
    "num_decoder_layers": 4,
    "nhead": 8,
    "encoder_dim_feedforward": null,
    "decoder_dim_feedforward": null,
    "backbone_kwargs": {}
  },
  "loss": {
    "name": "default",
    "task_aligner": "none",
    "weights": {}
  },
  "train": {
    "batch_size": 2,
    "lr": 0.0001,
    "epochs": 1
  }
}
