{
  "dataset": {
    "root": "../data/coco128",
    "split": "train2017",
    "format": "yolo"
  },
  "model": {
    "num_classes": 80,
    "hidden_dim": 256,
    "num_queries": 300,
    "use_uncertainty": false,
    "stem_channels": 32,
    "backbone_channels": [64, 128, 256],
    "stage_blocks": [1, 2, 2],
    "num_encoder_layers": 1,
    "num_decoder_layers": 3,
    "nhead": 8,
    "encoder_dim_feedforward": null,
    "decoder_dim_feedforward": null
  },
  "train": {
    "batch_size": 2,
    "lr": 0.0001,
    "epochs": 1
  }
}
