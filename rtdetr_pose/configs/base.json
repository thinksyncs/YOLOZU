{
  "dataset": {
    "root": "../data/coco128",
    "split": "train2017",
    "format": "yolo"
  },
  "model": {
    "num_classes": 80,
    "hidden_dim": 256,
    "num_queries": 300,
    "use_uncertainty": false,
    "backbone_channels": [64, 128, 256],
    "num_decoder_layers": 3,
    "nhead": 8
  },
  "train": {
    "batch_size": 2,
    "lr": 0.0001,
    "epochs": 1
  }
}
